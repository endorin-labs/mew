Description=Nitro Enclaves vsock Proxy
After=network-online.target
DefaultDependencies=no

[Service]
Type=simple
StandardOutput=journal
StandardError=journal
SyslogIdentifier=vsock-proxy
# Use RUST_LOG=trace for more verbose logging
ExecStart=/usr/bin/vsock-proxy 8001 aws-0-us-east-1.pooler.supabase.com 5432 --config /etc/nitro_enclaves/vsock-proxy.yaml
Restart=always
TimeoutSec=0

[Install]
WantedBy=multi-user.target